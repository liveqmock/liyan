    <PropertyDef name="columnSize">
      <Property name="dataType">String</Property>
      <Property name="label">长度</Property>
      <Validator type="custom">
        <ClientEvent name="onValidate">var columnType = arg.entity.get('columnType');
columnSize = arg.data;
/**
 * CHAR VARCHAR 类型校验 
 */
if ((columnType.indexOf("VARCHAR") &gt; -1 
		||columnType.indexOf("CHAR") &gt; -1)
		&amp;&amp;columnType.indexOf("VARCHAR () FOR BIT DATA")==-1
		&amp;&amp;columnType.indexOf("LONG VARCHAR FOR BIT DATA")==-1
		&amp;&amp;columnType.indexOf("LONG VARCHAR")==-1
		&amp;&amp;columnType.indexOf("CHAR () FOR BIT DATA")==-1){
	if(columnSize){
		var charReg = /^[1-9][0-9]{1,3}$/;		
		var res = columnSize.match(charReg);
		if(!res){
			arg.result="格式错误，必须为1-4位数字并且第1位数字不能为0！正确格式:【255】";
		}
	}else{
		arg.result="字段长度不能为空！";		
	}
	if(arg.entity.getOldData()&amp;&amp;arg.entity.getOldData().columnSize){
		if(parseInt(columnSize) &lt; parseInt(arg.entity.getOldData().columnSize)){
			arg.result="不能减少字段长度，只能扩展长度！";
		}
	}
/**
 * NUM DECIMAL 类型校验
 */
}else if (columnType.indexOf("NUMERIC") &gt; -1||columnType.indexOf("DECIMAL") &gt;-1) {
	if(columnSize){
		var numReg = /(^[1-9]{0,1}[0-9]{0,1},[1-9]$)|(^[1-9]$|[1-9][0-9]{0,1}$)/;
		var res = columnSize.match(numReg);
		if(!res){
			arg.result="格式错误，正确格式:【12,2或12】！";
		}
	}	
}else if(columnType.indexOf("CLOB") &gt; -1
		||columnType.indexOf("BLOB") &gt; -1){
	if(columnSize){
		var charReg = /^[1-9][0-9]{1,9}$/;		
		var res = columnSize.match(charReg);
		if(!res){
			arg.result="格式错误，必须为1-10位数字并且第1位数字不能为0！正确格式:【255】";
		}
	}
	if(arg.entity.getOldData()&amp;&amp;arg.entity.getOldData().columnSize){
		if(parseInt(columnSize) &lt; parseInt(arg.entity.getOldData().columnSize)){
			arg.result="不能减少字段长度，只能扩展长度！";
		}
	}
}




</ClientEvent>
      </Validator>
