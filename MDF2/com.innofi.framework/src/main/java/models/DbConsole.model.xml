    </PropertyDef>
    <PropertyDef name="columnName">
      <Property name="dataType">String</Property>
      <Property name="label">列名称</Property>
      <Validator type="regExp">
        <Property name="resultMessage">字段名必须只能字母开头+数字或下划线2-20位！</Property>
        <Property name="whiteRegExp">^[a-zA-Z][a-zA-Z0-9|_]{1,19}$</Property>
      </Validator>
    </PropertyDef>
    <PropertyDef name="columnType">
      <Property name="dataType">String</Property>
      <Property name="label">数据类型</Property>
    </PropertyDef>
    <PropertyDef name="columnSeq">
