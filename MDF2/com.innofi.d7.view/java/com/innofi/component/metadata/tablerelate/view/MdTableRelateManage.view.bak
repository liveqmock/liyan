	entity.fromJSON(JSON.parse(arg.selectedValue));&#xD;
	var dataSet = view.get(&quot;#dataSetMdTableRelate&quot;);&#xD;
	dataSet.getData(&quot;#&quot;).set(&quot;mainTableId&quot;,entity.get(&quot;id&quot;));&#xD;
	dataSet.getData(&quot;#&quot;).set(&quot;mainTableName&quot;,entity.get(&quot;tableName&quot;));&#xD;
}&#xD;
arg.selectedValue = undefined;&#xD;
return false;</ClientEvent>
