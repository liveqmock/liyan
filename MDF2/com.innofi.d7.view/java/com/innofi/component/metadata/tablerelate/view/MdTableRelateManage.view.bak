	dataSet.getData(&quot;#&quot;).set(code,entity.get(&quot;orgCode&quot;));&#xD;
	dataSet.getData(&quot;#&quot;).set(name,entity.get(&quot;orgName&quot;));&#xD;
	&#xD;
	dataSet.notifyObservers();&#xD;
}&#xD;
arg.selectedValue = undefined;&#xD;
